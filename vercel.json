
{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/unity/(.*)",
      "dest": "/public/unity/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}